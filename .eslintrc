{
  "extends": "airbnb",
  "env": {
    "node": true,
    "browser": true,
    "worker": true,
    "serviceworker": true
  },
  "globals": {
    "toolbox": true,
    "moment": true
  },
  "rules": {
    "no-warning-comments": 0,
    "global-require": 0,
    "arrow-parens": ["error", "as-needed"],
    "curly": 0
  }
}
